class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        n = len(prices) 
        if n < 2:
            return 0
        
        max_profit = 0

        for i in range(0, n):
            lowest = prices[i]
            for j in range(i+1,n):
                profit = prices[j] - lowest 
                if profit > max_profit:
                    max_profit = profit       
        return max_profit